<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_set_points" model="ir.cron">
        <field name="name">Set points for leagues</field>
        <field name="model_id" ref="model_sport_league"/>  
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="state">code</field>
        <field name="code">model._cron_set_points()</field>
        <field name="user_id" ref="base.user_root"/>
    </record>

    <record id="ir_cron_delete_unused_tags" model="ir.cron">
        <field name="name">Delete tags that are not in use</field>
        <field name="model_id" ref="model_sport_issue"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="state">code</field>
        <field name="code">model._cron_delete_unused_tags()</field>
        <field name="user_id" ref="base.user_root"/>
    </record>
</odoo>
