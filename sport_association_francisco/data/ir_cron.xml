<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sport_leage_set_score_cron" model="ir.cron">
        <field name="name">Set score</field>
        <field name="model_id" ref="model_sport_leage"/>
        <field name="active" eval="True"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="state">code</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="code">model.update_all_leage_points()</field>
    </record>
    <record id="sport_tag_cron" model="ir.cron">
        <field name="name">Remove unused tags</field>
        <field name="model_id" ref="model_sport_tag"/>
        <field name="active" eval="True"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="state">code</field>
        <field name="code">model.unlink_if_not_used()</field>
    </record>
</odoo>
