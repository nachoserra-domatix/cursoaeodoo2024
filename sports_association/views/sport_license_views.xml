<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="sport_license_view_tree" model="ir.ui.view">
        <field name="name">sport_license.view.tree</field>
        <field name="model">sport.license</field>
        <field name="arch" type="xml">
           <tree editable="bottom" string="License">
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id="sport_license_action" model="ir.actions.act_window">
        <field name="name">Sport License</field>
        <field name="res_model">sport.license</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create a new license
            </p>
        </field>
    </record>
    </odoo>