<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Menus -->
    <menuitem id="sport_association_menu"
        name="Sport Association"
        sequence="10"
        web_icon="calendar,static/description/icon.png"
        groups="base.group_user">

        <menuitem id="sport_issues_menu"
            name="Issues"
            sequence="20">

            <menuitem id="sport_issue_menu"
                name="Sport Issues"
                action="action_sport_issue"
                groups="base.group_user"
                sequence="21"/>

            <menuitem id="sport_issue_tag_menu"
                name="Issues Tags"
                action="action_sport_issue_tag"
                groups="base.group_user"
                sequence="22"/>

        </menuitem>        
        
        <menuitem id="sport_clinic_menu"
            name="Clinics"
            sequence="30"
            action="action_sport_clinic"            
            groups="base.group_user"/>        
        
        <menuitem id="sport_menu"
            name="Associated"
            groups="base.group_user"
            sequence="40">

            <menuitem id="sport_sport_menu"
                action="action_sport_sport"/>

            <menuitem id="sport_team_menu"
                name="Teams"                
                action="action_sport_team"            
                groups="base.group_user"/>
                
            <menuitem id="sport_player_menu"
                name="Players"                
                action="action_sport_player"            
                groups="base.group_user"/>
            
            <menuitem id="sport_licenses_menu"
                name="Licenses"                
                action="action_sport_license"            
                groups="base.group_user"/>

        </menuitem>
        
    </menuitem>

</odoo>