<?xml version='1.0' encoding='utf-8' ?>
<odoo>
    <record id="seq_sport_ticket" model="ir.sequence">
        <field name="name">Sport Ticket Sequence</field>
        <field name="code">sport.ticket</field>
        <field name="prefix">Ticket</field>
        <field name="padding">5</field>
    </record>
    <record id="seq_sport_issue" model="ir.sequence">
        <field name="name">Sport Issue Sequence</field>
        <field name="code">sport.issue</field>
        <field name="prefix">Issue #</field>
        <field name="padding">5</field>
    </record>
</odoo>
