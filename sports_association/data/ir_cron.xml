<?xml version='1.0' encoding='utf-8' ?>
<odoo>
    <record id="ir_cron_compute_classification" model="ir.cron">
        <field name="name">Cron Compute Classification</field>
        <field name="model_id" ref="model_sport_league" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="user_id" ref="base.user_root" />
        <field name="numbercall">-1</field>
        <field name="state">code</field>
        <field name="code">model._cron_set_classification()</field>
    </record>

    <record id="ir_cron_remove_unused_issue_tags" model="ir.cron">
        <field name="name">Cron Remove Unused Issue Tags</field>
        <field name="model_id" ref="model_sport_issue_tag" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="user_id" ref="base.user_root" />
        <field name="numbercall">-1</field>
        <field name="state">code</field>
        <field name="code">model._cron_remove_unused_tags()</field>
    </record>
</odoo>
