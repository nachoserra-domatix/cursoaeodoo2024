<odoo>
    <record id="ir_cron_set_score_leagues" model="ir.cron">
        <field name="name">Set score for leagues</field>
        <field name="model_id" ref="model_sport_league" />
        <field name="state">code</field>
        <field name="code">model._cron_set_score()</field>
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
    </record>

     <record id="ir_cron_delete_unused_issue_tags" model="ir.cron">
        <field name="name">Delete unused issue tags</field>
        <field name="model_id" ref="model_sport_issue" />
        <field name="state">code</field>
        <field name="code">model._cron_delete_unused_issue_tags()</field>
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
    </record>
</odoo>
