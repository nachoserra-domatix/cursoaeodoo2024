<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
    
        <!-- create data example for model sport.license-->
        <record id="sport_license_data_1" model="sport.license">
            <field name="name">License 1</field>
            <field name="reference">999-999-111-AK</field>
            <field name="start_date" eval="time.strftime('%Y-%m-%d %H:%M:%S')"></field>
            <field name="partner_id" ref="base.user_root"></field>
        </record>
        <record id="sport_license_data_2" model="sport.license">
            <field name="name">License 2</field>
            <field name="reference">999-999-222-AK</field>
            <field name="start_date">2024-02-02</field>
            <field name="partner_id" ref="base.user_root"></field>
        </record>
    
    </data>
    

</odoo>
