    <menuitem
        id="sport_menu"
        name="Sport Association"
        sequence="10">

        <menuitem 
            id="sport_menu_list"
            name="Sport Association"
            sequence="10">

            <menuitem 
                id="sport_issue_menu"
                name="Sport Issues"
                sequence="10"
                action="sports_association_sergio.action_sport_issue">
            </menuitem>
            <menuitem 
                id="sport_license_menu"
                name="Sport Licenses"
                sequence="20"
                action="sports_association_sergio.action_sport_license">
            </menuitem>      
            <menuitem 
                id="sport_clinic_menu"
                name="Sport Clinics"
                sequence="30"
