<?xml version="1.0" encoding="utf-8"?>
<odoo>

<record id="sport_ticket_view_tree" model="ir.ui.view">
    <field name="name">sport.ticket.view.tree</field>
    <field name="inherit_id" ref="sports_association_jlsanz.sport_ticket_view_tree" />
    <field name="model">sport.ticket</field>
    <field name="arch" type="xml">
        <field name="match_id" position="after">
            <field name="sale_order_id" />
        </field>
    </field>
</record>

<record id="sport_ticket_view_form" model="ir.ui.view">
    <field name="name">sport.ticket.view.form</field>
    <field name="inherit_id" ref="sports_association_jlsanz.sport_ticket_view_form" />
    <field name="model">sport.ticket</field>
    <field name="arch" type="xml">
        <field name="match_id" position="after">
            <field name="sale_order_id" />
        </field>
    </field>
</record>

</odoo>
